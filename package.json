    "@types/react-native": "0.70.6",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "jest-expo": "46.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
