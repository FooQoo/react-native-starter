    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "13.0.0",
